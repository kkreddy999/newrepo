def is_valid_parentheses(s):
    stack =[]
    for char in s:
        if char in '({[':
            stack.append(char)
        elif char in ')}]':
            if not stack:
                return False
            last = stack.pop()
            if char == ')' and last != '(':
                return False
            if char == '}' and last != '{':
                return False
            if char == ']' and last != '[':
                return False
   
    return len(stack) == 0
 
input_string = input("Enter a string to check: ")
print(is_valid_parentheses(input_string))
 
 